{
  "name": "lmorandomizer",
  "description": "Randomizer for La-Mulana Original.",
  "version": "0.2.1",
  "bugs": {
    "url": "https://github.com/progre/lmoriginalrandomizer/issues"
  },
  "build": {
    "directories": {
      "output": "output"
    },
    "files": [
      "dist",
      "LICENSE",
      "README.md"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "iconv-lite": "^0.4.24",
    "js-sha3": "^0.8.0",
    "js-yaml": "^3.12.0",
    "parse5": "^5.1.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "seedrandom": "^2.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@progre/tslint-config": "~5.10.0",
    "@progre/webpack-config": "^0.0.6",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^8.10.29",
    "@types/parse5": "^5.0.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/seedrandom": "^2.4.27",
    "del-cli": "^1.1.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "source-map-support": "^0.5.9",
    "ts-loader": "^5.0.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "homepage": "https://github.com/progre/lmoriginalrandomizer#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progre/lmoriginalrandomizer.git"
  },
  "scripts": {
    "build": "run-s clean wasm-build release-build",
    "clean": "del-cli dist tmp",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "debug-build": "webpack --mode development --progress",
    "dev": "webpack --mode development --progress --watch",
    "test": "npx -q -p typescript -p ts-node ts-node src/test/test.ts",
    "package": "electron-builder --win zip --mac zip --publish onTag",
    "release-build": "webpack --mode production --progress",
    "start": "electron .",
    "wasm-build": "rustc --target wasm32-unknown-unknown -O src/lib.rs --crate-type=cdylib --out-dir dist/public/wasm"
  },
  "typings": "dist/index.d.ts"
}
